{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "transform-runtime"
  ],
  "env": {
    "start": {
      "plugins": [
        "react-transform"
      ]
    },
    "extra": {
      "react-transform": {
        "transforms": [
          {
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }
        ]
      }
    }
  }
}
